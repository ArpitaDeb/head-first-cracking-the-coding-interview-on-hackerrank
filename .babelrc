{
  "plugins": [
    "transform-object-rest-spread",
    "transform-runtime",
    "transform-async-to-generator"
  ],
  "presets": [
    "es2015",
    "react"
  ],
  "env": {
    "development": {
      "plugins": [
        [
          "react-transform",
          {
            "transforms": [
              {
                "transform": "livereactload/babel-transform",
                "imports": [
                  "react"
                ]
              }
            ]
          }
        ]
      ]
    }
  }
}
